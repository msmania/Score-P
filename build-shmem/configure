# _scorep_check_symbols_bisect LINENO TAG FOUND-SYMBOL-VAR START END
# ------------------------------------------------------------------
# The symbol check.
_scorep_check_symbols_bisect ()
{

    as_lineno=${as_lineno-"$1"} as_lineno_stack=as_lineno_stack=$as_lineno_stack
    if test $(($5 - $4 + 1)) -eq 0; then
        return
    fi

    cat <<_ACEOF >confsymbolstest.h
#ifdef __cplusplus
extern "C" {
#endif
_ACEOF

    sed -n -e "$4,$5 p" confsymbols >confsymbols.this
    while read symbol define; do
        cat <<_ACEOF
#undef $symbol
char $symbol ();
_ACEOF
    done <confsymbols.this >>confsymbolstest.h

    cat <<_ACEOF >>confsymbolstest.h
#ifdef __cplusplus
}
#endif
_ACEOF

    cat <<_ACEOF >confsymbolstest.c
return 0
_ACEOF
    while read symbol define; do
        cat <<_ACEOF
    | $symbol()
_ACEOF
    done <confsymbols.this >>confsymbolstest.c

    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include "confsymbolstest.h"

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
#ifdef FC_DUMMY_MAIN
#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{

#include "confsymbolstest.c"

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :

        # we successfully linked all symbols, add them to the confdefs.h
        while read symbol define; do
            { $as_echo "$as_me:${as_lineno-$LINENO}: checking for $symbol in $2" >&5
$as_echo_n "checking for $symbol in $2... " >&6; }
            { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "${as__grn}yes${as__std}" >&6; }

            cat >>confdefs.h <<_ACEOF
#define $define 1
_ACEOF


            if test -n "$3"; then :
  as_fn_append $3 " $symbol"
fi

        done <confsymbols.this


else


        # if this was only one symbol, then we can be sure, that this does not exist

        if test $(($5 - $4 + 1)) -eq 1; then

            while read symbol define; do

                { $as_echo "$as_me:${as_lineno-$LINENO}: checking for $symbol in $2" >&5
$as_echo_n "checking for $symbol in $2... " >&6; }
                { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "${as__red}no${as__std}" >&6; }

                cat >>confdefs.h <<_ACEOF
#define $define 0
_ACEOF


            done <confsymbols.this

        else # #symbols != 1

            # we need to bisect into lower and upper

            # no local variables, thus recompute mid point after first recursion

        _scorep_check_symbols_bisect "$LINENO" "$2" "$3" $4 $((($4 + ($5 - $4 + 1) / 2) - 1))


        _scorep_check_symbols_bisect "$LINENO" "$2" "$3" $(($4 + ($5 - $4 + 1) / 2)) $5


        fi # #symbols != 1


fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno

} # _scorep_check_symbols_bisect

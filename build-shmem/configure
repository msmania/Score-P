_LTEOF
chmod +x "$CONFIG_LT"

# configure is writing to config.log, but config.lt does its own redirection,
# appending to config.log, which fails on DOS, as config.log is still kept
# open by configure.  Here we exec the FD to /dev/null, effectively closing
# config.log, so it can be properly (re)opened and appended to by config.lt.
lt_cl_success=:
test yes = "$silent" &&
  lt_config_lt_args="$lt_config_lt_args --quiet"
exec 5>/dev/null
$SHELL "$CONFIG_LT" $lt_config_lt_args || lt_cl_success=false
exec 5>>config.log
$lt_cl_success || as_fn_exit 1


ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

_afs_lib_prevent_check=no
_afs_lib_download=no
{ _afs_lib_prevent_check_reason=; unset _afs_lib_prevent_check_reason;}

# Check whether --with-libbfd was given.
if test "${with_libbfd+set}" = set; then :
  withval=$with_libbfd;
fi


# Check whether --with-libbfd-include was given.
if test "${with_libbfd_include+set}" = set; then :
  withval=$with_libbfd_include;
fi


# Check whether --with-libbfd-lib was given.
if test "${with_libbfd_lib+set}" = set; then :
  withval=$with_libbfd_lib;
fi

# Supersede environment variables if --with-libLIBRARY-NAME option
# given. Unset env variables to prevent further processing.
if test "${with_libbfd:+set}" = set; then :
  if test "${LIBBFD_LIB:+set}" = set; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: LIBBFD_LIB=${LIBBFD_LIB} superseded with --with-libbfd=${with_libbfd}" >&5
$as_echo "$as_me: ${as__yel}WARNING: LIBBFD_LIB=${LIBBFD_LIB} superseded with --with-libbfd=${with_libbfd}${as__std}" >&2;}
          { LIBBFD_LIB=; unset LIBBFD_LIB;}
fi
          if test "${LIBBFD_INCLUDE:+set}" = set; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: LIBBFD_INLUDE=${LIBBFD_INCLUDE} superseded with --with-libbfd=${with_libbfd}" >&5
$as_echo "$as_me: ${as__yel}WARNING: LIBBFD_INLUDE=${LIBBFD_INCLUDE} superseded with --with-libbfd=${with_libbfd}${as__std}" >&2;}
          { LIBBFD_INCLUDE=; unset LIBBFD_INCLUDE;}
fi
fi
# Convert LIB<upcase(LIBRARY_NAME)>_(LIB|INCLUDE) env var to
# --with-libLIBRARY-NAME-(lib|include) if the latter wasn't
# provided. Otherwise, --with-libLIBRARY-NAME-(lib|include)
# supersedes.
if test "${LIBBFD_LIB:+set}" = set; then :
  if test "${with_libbfd_lib:+set}" = set; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: LIBBFD_LIB=${LIBBFD_LIB} superseded with --with-libbfd-lib=${with_libbfd_lib}" >&5
$as_echo "$as_me: ${as__yel}WARNING: LIBBFD_LIB=${LIBBFD_LIB} superseded with --with-libbfd-lib=${with_libbfd_lib}${as__std}" >&2;}
else
  with_libbfd_lib="${LIBBFD_LIB}"
          { $as_echo "$as_me:${as_lineno-$LINENO}: info: Using LIBBFD_LIB as --with-libbfd-lib=${with_libbfd_lib}. Further mentioning of --with-libbfd-lib applies to LIBBFD_LIB." >&5
$as_echo "$as_me: ${as__blu}info: Using LIBBFD_LIB as --with-libbfd-lib=${with_libbfd_lib}. Further mentioning of --with-libbfd-lib applies to LIBBFD_LIB.${as__std}" >&2;}
fi
fi
if test "${LIBBFD_INCLUDE:+set}" = set; then :
  if test "${with_libbfd_include:+set}" = set; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: LIBBFD_INCLUDE=${LIBBFD_INCLUDE} superseded with --with-libbfd-include=${with_libbfd_include}" >&5
$as_echo "$as_me: ${as__yel}WARNING: LIBBFD_INCLUDE=${LIBBFD_INCLUDE} superseded with --with-libbfd-include=${with_libbfd_include}${as__std}" >&2;}
else
  with_libbfd_include="${LIBBFD_INCLUDE}"
          { $as_echo "$as_me:${as_lineno-$LINENO}: info: Using LIBBFD_INCLUDE as --with-libbfd-include=${with_libbfd_include}. Further mentioning of --with-libbfd-include applies to LIBBFD_INCLUDE." >&5
$as_echo "$as_me: ${as__blu}info: Using LIBBFD_INCLUDE as --with-libbfd-include=${with_libbfd_include}. Further mentioning of --with-libbfd-include applies to LIBBFD_INCLUDE.${as__std}" >&2;}
fi
fi
# Check valid combinations of options and if directories and libs exist if <path> was provided.
case "${with_libbfd:+set1}${with_libbfd_lib:+set2}${with_libbfd_include:+set3}" in #(
      set1set*) :
    as_fn_error $? "Use either shorthand --with-libbfd or explicit options --with-libbfd-lib and --with-libbfd-include." "$LINENO" 5 ;; #(
  set2) :
    as_fn_error $? "If --with-libbfd-lib is given, --with-libbfd-include is required." "$LINENO" 5 ;; #(
  set3) :
    as_fn_error $? "If --with-libbfd-include is given, --with-libbfd-lib is required." "$LINENO" 5 ;; #(
  set1) :
    case $with_libbfd in #(
  yes) :
    if test "x${afs_lib_require_install_paths}" = xyes; then :
  with_libbfd=no; with_libbfd_lib=no; with_libbfd_include=no
                          _afs_lib_prevent_check=yes
                          _afs_lib_prevent_check_reason="crosscompile"
                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: In cross-compile mode, you need to provide a path to --with-libbfd (or --with-libbfd-lib and --with-libbfd-lib-include) in order to activate bfd support. Alternatively, use --with-libbfd=download." >&5
$as_echo "$as_me: ${as__yel}WARNING: In cross-compile mode, you need to provide a path to --with-libbfd (or --with-libbfd-lib and --with-libbfd-lib-include) in order to activate bfd support. Alternatively, use --with-libbfd=download.${as__std}" >&2;}
else
  with_libbfd_lib=yes; with_libbfd_include=yes
fi ;; #(
  no) :
    with_libbfd_lib=no; with_libbfd_include=no
                    _afs_lib_prevent_check=yes
                    _afs_lib_prevent_check_reason="disabled" ;; #(
  download) :
    with_libbfd_lib=download; with_libbfd_include=download; _afs_lib_download=yes ;; #(
  *) :

for ac_var in with_libbfd
do
  eval ac_val=\$$ac_var
  # Remove trailing slashes.
  case $ac_val in
    */ )
      ac_val=`expr "X$ac_val" : 'X\(.*[^/]\)' \| "X$ac_val" : 'X\(.*\)'`
      eval $ac_var=\$ac_val;;
  esac
  # Be sure to have absolute directory names.
  case $ac_val in
    [\\/$]* | ?:[\\/]* )  continue;;
    NONE | '' ) case $ac_var in *prefix ) continue;; esac;;
  esac
  as_fn_error $? "Expected an absolute directory name for --with-libbfd but got $ac_val." "$LINENO" 5
done

              if ! test -d "$with_libbfd"; then :
  as_fn_error $? "Directory $with_libbfd (from --with-libbfd) does not exist. Typo?" "$LINENO" 5
fi
                                          if ! test -d "$with_libbfd/include"; then :
  as_fn_error $? "Directory $with_libbfd/include (via --with-libbfd) does not exist. Consider using --with-libbfd-lib and --with-libbfd-include." "$LINENO" 5
fi
              for header in bfd.h; do
                  if ! test -r "$with_libbfd/include/$header"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Directory $with_libbfd/include (via --with-libbfd) does not contain $header. Consider using --with-libbfd-lib and --with-libbfd-include." >&5
$as_echo "$as_me: ${as__yel}WARNING: Directory $with_libbfd/include (via --with-libbfd) does not contain $header. Consider using --with-libbfd-lib and --with-libbfd-include.${as__std}" >&2;}
                       header_consistent=no
fi
              done
              if test "x${header_consistent}" = xno; then :
  as_fn_error $? "Header(s) check failed, see WARNING(s) above." "$LINENO" 5
fi
              with_libbfd_include="$with_libbfd/include"
                            if test -d "${with_libbfd}/lib64"; then :
  if test "$(echo $with_libbfd/lib64/libbfd.*)" != "$with_libbfd/lib64/libbfd.*"; then :
  with_libbfd_lib="${with_libbfd}/lib64"
fi
elif test -d "${with_libbfd}/lib"; then :
  if test "$(echo $with_libbfd/lib/libbfd.*)" != "$with_libbfd/lib/libbfd.*"; then :
  with_libbfd_lib="${with_libbfd}/lib"
fi
else
  as_fn_error $? "Neither ${with_libbfd}/(lib64|lib) (via --with-libbfd) exist. Consider using --with-libbfd-lib and --with-libbfd-include." "$LINENO" 5
fi
              if test "${with_libbfd_lib:+set}" != set; then :
  as_fn_error $? "Neither ${with_libbfd}/(lib64|lib) contain libbfd.*. Consider using --with-libbfd-lib and --with-libbfd-include." "$LINENO" 5
fi ;;
esac ;; #(
      set2set3) :
    case ${with_libbfd_lib}${with_libbfd_include} in #(
  *yes*|*no*) :
    as_fn_error $? "Both, --with-libbfd-lib and --with-libbfd-include require a <path>." "$LINENO" 5 ;; #(
  *) :

for ac_var in with_libbfd_include
do
  eval ac_val=\$$ac_var
  # Remove trailing slashes.

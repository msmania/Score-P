	  archive_expsym_cmds_FC='$CC -shared $pic_flag -o $lib $libobjs $deplibs $compiler_flags $wl-retain-symbols-file,$export_symbols'
	  hardcode_libdir_flag_spec_FC='$wl-rpath,$libdir'
	  export_dynamic_flag_spec_FC='$wl-E'
	else

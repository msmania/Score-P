    $as_echo "${as__red}$lt_cv_prog_compiler_pic_FC${as__std}" >&6 ;; #(
  *) :
    $as_echo "${as__blu}$lt_cv_prog_compiler_pic_FC${as__std}" >&6 ;;
esac; }

            eventInfo->data_event.device_ptr;
          case acc_ev_wait_start:
            return;
      }

      // check for used other event fields
      eventInfo->other_event.tool_info;
      eventInfo->other_event.implicit;
    }

void
acc_register_library( acc_prof_reg accRegister,

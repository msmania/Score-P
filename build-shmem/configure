  as__color=no;
elif test "X$AS_COLOR_FORCE" = X1; then
  as__color=yes
elif { test "X$TERM" != Xdumb || test "X$AS_COLOR" = X1; } && { test -t 1; } 2>/dev/null; then
  as__color=yes
fi

  yes|ok|yes[\ ,]*) :
    $as_echo "${as__grn}$lt_cv_irix_exported_symbol${as__std}" >&6 ;; #(
  no|failed|unsupported|no[\ ,]*) :
    $as_echo "${as__red}$lt_cv_irix_exported_symbol${as__std}" >&6 ;; #(
  *) :
    $as_echo "${as__blu}$lt_cv_irix_exported_symbol${as__std}" >&6 ;;

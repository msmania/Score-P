  no|failed|unsupported|no[\ ,]*) :
    $as_echo "${as__red}$hardcode_action_FC${as__std}" >&6 ;; #(
  *) :
    $as_echo "${as__blu}$hardcode_action_FC${as__std}" >&6 ;;
esac; }


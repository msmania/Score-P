	;;
      esac
    fi
  fi

{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ld_shlibs_FC" >&5
case "$ld_shlibs_FC" in #(
  yes|ok|yes[\ ,]*) :
    $as_echo "${as__grn}$ld_shlibs_FC${as__std}" >&6 ;; #(
  no|failed|unsupported|no[\ ,]*) :
    $as_echo "${as__red}$ld_shlibs_FC${as__std}" >&6 ;; #(
  *) :
    $as_echo "${as__blu}$ld_shlibs_FC${as__std}" >&6 ;;

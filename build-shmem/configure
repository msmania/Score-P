    "libtool-fix-cray-libunwind") CONFIG_COMMANDS="$CONFIG_COMMANDS libtool-fix-cray-libunwind" ;;

  *) as_fn_error $? "invalid argument: \`$ac_config_target'" "$LINENO" 5;;
